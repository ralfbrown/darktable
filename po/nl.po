#: ../build/src/preferences_gen.h:1680
msgctxt "preferences"
msgid "1080p"
msgstr "1080p"

#: ../build/src/preferences_gen.h:1685
msgctxt "preferences"
msgid "WQXGA"
msgstr "WQXGA"

#: ../build/src/preferences_gen.h:1690
msgctxt "preferences"
msgid "4K"
msgstr "4K"

#: ../build/src/preferences_gen.h:1695
